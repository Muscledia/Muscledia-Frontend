{
  "expo": {
    "name": "Muscledia",
    "slug": "Muscledia",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/appIcon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/appIcon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      ["expo-notifications", {"mode": "production"}]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
